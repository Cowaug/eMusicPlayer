<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/red0</item>
        <item name="android:tint">@color/black</item>
        <item name="android:progressTint">@color/black</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:colorControlHighlight">?attr/colorAccent</item>
    </style>

    <!-- Default theme -->
    <style name="DefaultTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:statusBarColor">@color/darkThemeBackground</item>
        <item name="android:navigationBarColor">@color/darkThemeBackground</item>
    </style>

    <!-- Base Dark theme -->
    <style name="DarkTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:colorControlHighlight">?attr/colorAccent</item>
    </style>

